{
  "extends": ["stylelint-config-recommended-vue", "stylelint-config-recommended", "stylelint-config-standard"],
  "rules": {
    "string-quotes": "single",
    "selector-class-pattern": "[a-z]+([-_][a-z])+",
    "value-keyword-case": [
      "lower",
      {
        "ignoreKeywords": ["coverFit"]
      }
    ]
  }
}
